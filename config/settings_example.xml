<settings>
    <main> 
        <name>HABframe</name>
        <hostname>localhost</hostname>
        <port>8890</port>
        <openhab_ip></openhab_ip>
        <openhab_port>8080</openhab_port>
        <resize_icons>1</resize_icons>
        <sitemap></sitemap>
        <habpanel_link>off</habpanel_link>
        <enable_screen_control>off</enable_screen_control>
        <screen_on_url></screen_on_url>
        <screen_off_url></screen_off_url>
        <screen_on_cmd></screen_on_cmd>
        <screen_off_cmd></screen_off_cmd>
        <enable_clock>1</enable_clock>
        <enable_album>0</enable_album>
	<mqtt_control_topic>habframe/#</mqtt_control_topic>
    </main>
    <logging>
        <log>HABframe.log</log>
        <errorlog>HABframe_error.log</errorlog>
        <eventlog>HABframe_event.log</eventlog>
        <level>info</level>
        <rotation>3</rotation>
    </logging> 
    <album>
        <image_size></image_size>
        <random_picture_url></random_picture_url>
        <picture_info_url></picture_info_url>
    </album>
    <data>
        <location>data</location>
    </data>
    <page>
        <event_check_reload>2</event_check_reload>
        <page_reload>10</page_reload>
    </page>
    <messaging>
        <mqtt_topics>topic1,topic2,topicx</mqtt_topics>
    </messaging>
    <mqtt>
        <server>ip-address</server>
        <port>1883</port>
        <timeout>60</timeout>
    </mqtt>
</settings>
